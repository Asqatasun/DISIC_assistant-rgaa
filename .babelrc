{
	"plugins": [
		"lodash",
		"transform-object-rest-spread",
		"transform-function-bind",
		"transform-export-extensions"
	],
	"presets": [
		"react",
		["env", {
			"targets": {
				"chrome": 56,
				"firefox": 51
			},
			"exclude": [
				"transform-regenerator"
			]
		}]
	],
	"env": {
		"production": {
			"plugins": [
				"transform-react-constant-elements",
				"transform-react-inline-elements"
			]
		}
	}
}
